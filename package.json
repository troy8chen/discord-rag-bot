{
  "name": "discord-rag-bot",
  "version": "1.0.0",
  "description": "Discord bot interface for Inngest RAG ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t discord-rag-bot .",
    "docker:run": "docker run --env-file .env discord-rag-bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/discord-rag-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "rag",
    "typescript",
    "redis",
    "inngest"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
} 