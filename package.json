{
  "name": "discord-rag-bot",
  "version": "1.0.0",
  "description": "Discord bot that connects to a RAG service for AI-powered document assistance",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --project tsconfig.prod.json",
    "build:dev": "tsc --project tsconfig.json",
    "build:prod": "npm install --only=prod && npm install typescript @types/node @types/express @types/uuid && npm run build",
    "render-build": "npm ci && npm run build",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:redis": "node scripts/test-redis.js",
    "clean": "rm -rf dist",
    "setup": "chmod +x scripts/dev-setup.sh && ./scripts/dev-setup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/discord-rag-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "rag",
    "ai",
    "chatbot",
    "documentation",
    "typescript"
  ],
  "author": "Troy Chen",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "ioredis": "^5.3.2",
    "module-alias": "^2.2.3",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "@types/express": "^4.17.22",
    "@types/node": "^20.17.56",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
